{
  "name": "bitwave.tv",
  "version": "1.26.5",
  "description": "An open platform live streaming service for creators to freely express themselves.",
  "author": "Dispatch",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "dev:express": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:nuxt": "nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "start:express": "cross-env NODE_ENV=production node server/index.js",
    "start:nuxt": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "docker:build": "docker build -t bitwavetv/bitwave:latest .",
    "docker:push": "docker push bitwavetv/bitwave:latest",
    "rollout": "kubectl rollout deployment/bitwave-app -n bitwave-node",
    "rollout:restart": "kubectl rollout deployment/bitwave-app -n bitwave-node",
    "rollout:status": "kubectl rollout status deployment/bitwave-app -n bitwave-node"
  },
  "dependencies": {
    "@bitwave/chat-client": "^0.3.10",
    "@hcaptcha/vue-hcaptcha": "^0.3.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/component-cache": "^1.1.6",
    "@nuxtjs/device": "^2.0.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/manifest": "^3.0.0-beta.16",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sentry": "^5.0.3",
    "@nuxtjs/toast": "^3.3.1",
    "@sentry/browser": "^6.2.4",
    "@videojs/http-streaming": "^2.6.4",
    "babel-runtime": "^6.26.0",
    "braintree-web-drop-in": "^1.25.0",
    "chart.js": "^2.9.4",
    "cookie-universal-nuxt": "^2.1.4",
    "cross-env": "^7.0.3",
    "dashjs": "^3.2.1",
    "express": "^4.17.1",
    "firebase": "^8.3.1",
    "jwt-decode": "^3.1.2",
    "marked": "^0.7.0",
    "moment": "^2.29.1",
    "nuxt": "^2.14.12",
    "nuxt-stripe-module": "^2.1.0",
    "sass-loader": "^10.1.1",
    "socket.io-client": "^2.3.1",
    "video.js": "^7.11.4",
    "videojs-contrib-dash": "^4.1.0",
    "videojs-contrib-quality-levels": "^2.0.9",
    "videojs-hls-quality-selector": "^1.1.4",
    "videojs-youtube": "^2.6.1",
    "vue-awesome-countdown": "^1.1.4",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.1"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.3",
    "vue": "^2.6.12"
  }
}
